N := 100
l := 30

BEGIN :: types
r := ZR
mpk#1 := G2
S := G1
u := G1
pk := G1
sk := G1
END :: types

BEGIN :: precompute
h := H(M | L | u, ZR)
S := sk^(h + r)
END :: precompute

BEGIN :: constant
mpk#0, mpk#1
END :: constant

verify := { e((prod{y:=1, l} on (u_y * (pk_y^h_y))) , mpk#0) == e(S, mpk#1) }

N := 100
l := 2

BEGIN :: types
M := ZR
t := ZR
Atpk := G2
Btpk := G2
Ctpk := G2
D := GT
S := G1
END :: types

BEGIN :: precompute
D := e(g1, g2)
END :: precompute

BEGIN :: constant
Atpk, Btpk, Ctpk, D
END :: constant

verify :=  { prod{y:=1, l} on e(S_y, Atpk_y * (Btpk_y^M_y) * (Ctpk_y^t_y)) } == D

BEGIN :: transform
2, 3, 4, S, 3
END :: transform
